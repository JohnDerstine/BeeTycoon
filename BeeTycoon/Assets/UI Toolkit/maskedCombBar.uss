.unity-progress-bar__container {
    max-height: none;
    height: 300px;
    width: 250px;
    rotate: 270deg;
}

.unity-progress-bar__progress {
    background-color: rgba(0, 0, 0, 0);
    -unity-background-scale-mode: scale-and-crop;
}

#CombBar .unity-progress-bar__progress {
    background-image: url("project://database/Assets/Sprites/honeycomb-pattern-background-4.png?fileID=21300000&guid=a412bcd8228d57e46a41851eac51290f&type=3#honeycomb-pattern-background-4");
}

#HoneyBar .unity-progress-bar__progress {
    background-image: url("project://database/Assets/Sprites/79d3643365bfbb63e96a0b6289d8e58c.jpg?fileID=21300000&guid=878be5b2dbe6ff241a20eab578abc40d&type=3#79d3643365bfbb63e96a0b6289d8e58c");
}

#NectarBar .unity-progress-bar__progress {
